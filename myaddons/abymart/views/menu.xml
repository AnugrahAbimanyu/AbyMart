<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <!-- This Menu Item will appear in the Upper bar, that's why It needs NO parent or action -->
    <menuitem
        id="abymart_menu_root"
        name="Aby Mart"
        sequence="10"/>

    <!-- This Menu Item Must have a parent -->
    <menuitem
        id="abymart_barang_menu_categ"
        name="Barang"
        parent="abymart_menu_root"
        sequence="10"/>

    <!-- This Menu Item Must have a parent -->
    <menuitem
        id="abymart_pegawai_menu_categ"
        name="Pegawai"
        parent="abymart_menu_root"
        sequence="20"/>

    <!-- This Menu Item Must have a parent -->
    <menuitem
	  id="abymart_transaksi_menu_categ"
	  name="Transaksi"
	  parent="abymart_menu_root"
	  sequence="42"/>
    
</odoo>